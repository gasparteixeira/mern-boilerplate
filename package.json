{
  "name": "mern-boilerplate",
  "version": "1.0.0",
  "description": "A fullstack boilerplate project",
  "repository": "https://github.com/gasparteixeira/mern-boilerplate",
  "author": "Gaspar Teixeira <gaspar.teixeira@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "dev": "webpack --mode development --module-bind js=babel-loader",
    "build": "webpack --mode production --module-bind js=babel-loader",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./client/src/__tests__/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "./client/src/__tests__/setup/"
    ]
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "evil-icons": "^1.10.1",
    "lodash": "^4.17.11",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "reactstrap": "^6.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.1",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "interpolate-html-plugin": "^3.0.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "redux-mock-store": "^1.5.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9"
  }
}
